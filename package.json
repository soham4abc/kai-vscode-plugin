{
    "name": "kai-vscode-plugin",
    "displayName": "Kai",
    "description": "Konveyor AI",
    "version": "0.0.3",
    "license": "epl-2.0",
    "publisher": "konveyor",
    "author": "Konveyor",
    "theme": "kai",
    "preview": true,
    "icon": "resources/konveyor-icon-color.png",
    "homepage": "https://github.com/konveyor-ecosystem/kai-vscode-plugin/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/konveyor-ecosystem/kai-vscode-plugin"
    },
    "bugs": "https://github.com/konveyor-ecosystem/kai-vscode-plugin/issues",
    "engines": {
        "vscode": "^1.59.1"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Migration",
        "Migrate",
        "MTA",
        "RHAMT",
        "MTA",
        "JBoss",
        "Analyze",
        "Kai"
    ],
    "activationEvents": [
        "onCommand:rhamt.createConfiguration",
        "onCommand:rhamt.newRuleset",
        "onCommand:rhamt.showRhamtExplorer",
        "onView:rhamtExplorerView",
        "onWebviewPanel:rhamtConfigurationEditor"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./ && cd resources/pre && tsc -p ./",
        "watch": "tsc -watch -p ./",
        "watch-pre": "tsc -watch -p ./resources/pre",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "category": "MTA",
                "command": "rhamt.newConfiguration",
                "title": "New Configuration",
                "icon": {
                    "light": "resources/light/add.svg",
                    "dark": "resources/dark/add-inverse.svg"
                }
            },
            {
                "category": "MTA",
                "command": "rhamt.modelReload",
                "title": "Refresh Configurations",
                "icon": "$(refresh)"
            },
            {
                "category": "MTA",
                "command": "rhamt.showRhamtExplorer",
                "title": "Show Explorer"
            },
            {
                "category": "MTA",
                "command": "rhamt.runConfiguration",
                "title": "Run",
                "enablement": "cli-enabled",
                "icon": "$(run)"
            },
            {
                "category": "MTA",
                "command": "rhamt.rerun",
                "title": "Rerun"
            },
            {
                "category": "MTA",
                "command": "rhamt.runProviders",
                "title": "Run Provider"
            },
            {
                "category": "MTA",
                "command": "rhamt.deleteConfiguration",
                "title": "Delete",
                "icon": "$(trash)"
            },
            {
                "category": "MTA",
                "command": "rhamt.openConfiguration",
                "title": "Open"
            },
            {
                "category": "MTA",
                "command": "rhamt.openReportExternal",
                "title": "Open Report"
            },
            {
                "category": "MTA",
                "command": "rhamt.openDoc",
                "title": "Open Code"
            },
            {
                "category": "MTA",
                "command": "rhamt.openIssueDetails",
                "title": "View Details"
            },
            {
                "category": "MTA",
                "command": "rhamt.refreshResults",
                "title": "Refresh"
            },
            {
                "category": "MTA",
                "command": "rhamt.deleteIssue",
                "title": "Delete"
            },
            {
                "category": "MTA",
                "command": "rhamt.markIssueAsComplete",
                "title": "Mark as Complete"
            },
            {
                "category": "MTA",
                "command": "rhamt.markIssueAsIncomplete",
                "title": "Mark as Incomplete"
            },
            {
                "category": "MTA",
                "command": "rhamt.deleteResults",
                "title": "Delete Analysis Results"
            },
            {
                "category": "MTA",
                "command": "rhamt.newRuleset",
                "title": "New Ruleset"
            },
            {
                "category": "MTA",
                "command": "rhamt.applyQuickfix",
                "title": "Apply Quickfix"
            },
            {
                "category": "MTA",
                "command": "rhamt.acceptChanges",
                "title": "Apply Changes",
                "icon": "$(check)"
            },
            {
                "category": "MTA",
                "command": "rhamt.rejectChanges",
                "title": "Reject Changes",
                "icon": "$(x)"
            },
            {
                "category": "MTA",
                "command": "rhamt.applyQuickfixes",
                "title": "Apply All Quickfixes"
            },
            {
                "command": "rhamt.showCustomPanel",
                "title": "Show Custom Panel"
            },
            {
                "command": "rhamt.showSplitWebview",
                "title": "Show Splits Webview"
            },
            {
                "category": "MTA",
                "command": "rhamt.previewQuickfix",
                "title": "Preview Quickfix"
            },
            {
                "category": "MTA",
                "command": "rhamt.activate",
                "title": "Activate Analysis"
            },
            {
                "category": "MTA",
                "command": "rhamt.deactivate",
                "title": "Deactivate Analysis"
            },
            {
                "category": "MTA",
                "command": "rhamt.kai",
                "title": "Generate Kai-Fix"
            },
            {
                "category": "MTA",
                "command": "rhamt.Kai-Fix-Files",
                "title": "Kai-Fix All"
            },
            {
                "category": "MTA",
                "command": "rhamt.Stop",
                "title": "Stop Process"
            }
        ],
        "configuration": [
            {
                "title": "CLI",
                "properties": {
                    "cli.executable.path": {
                        "type": "string",
                        "default": "",
                        "description": "Specifies absolute path to the executable (e.g., /path/to/cli)",
                        "scope": "window"
                    }
                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "rhamt-explorer",
                    "title": "KAI",
                    "icon": "resources/konveyor-icon-color.png"
                },
                {
                    "id": "kaiFixDetailsSidebar",
                    "title": "Kai Fix Details",
                    "icon": "media/sidebar-icon.svg"
                }
            ],
            "panel": [
                {
                    "id": "myCustomViewContainer",
                    "title": "Kaifix by MTA",
                    "icon": "resources/konveyor-icon-color.png"
                }
            ]
        },
        "views": {
            "rhamt-explorer": [
                {
                    "id": "rhamtExplorerView",
                    "name": "Explorer"
                },
                {
                    "type": "webview",
                    "id": "myWebView",
                    "name": "Kai Fix Actions",
                    "visibility": "visible"
                }
            ],
            "myCustomViewContainer": [
                {
                    "id": "myCustomView",
                    "name": "Accept/Reject Changes"
                }
            ],
            "kaiFixDetailsSidebar": [
                {
                    "id": "kaiFixButtons",
                    "name": "Actions"
                }
            ]
        },
        "snippets": [
            {
                "language": "xml",
                "path": "./rule-snippets/xml.json"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "rhamt.newConfiguration",
                    "when": "view == rhamtExplorerView",
                    "group": "navigation"
                },
                {
                    "command": "rhamt.modelReload",
                    "when": "view == rhamtExplorerView",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "rhamt.runConfiguration",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "rhamt"
                },
                {
                    "command": "rhamt.runConfiguration",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "inline"
                },
                {
                    "command": "rhamt.deleteConfiguration",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "z"
                },
                {
                    "command": "rhamt.deleteConfiguration",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "inline"
                },
                {
                    "command": "rhamt.openConfiguration",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "rhamt"
                },
                {
                    "command": "rhamt.refreshResults",
                    "when": "view == rhamtExplorerView && viewItem =~ /rhamtConfiguration/i",
                    "group": "rhamt"
                },
                {
                    "command": "rhamt.openReportExternal",
                    "when": "view == rhamtExplorerView && viewItem == report",
                    "group": "rhamt@1"
                },
                {
                    "command": "rhamt.openDoc",
                    "when": "view == rhamtExplorerView && viewItem =~ /issue/i",
                    "group": "rhamt@0"
                },
                {
                    "command": "rhamt.openIssueDetails",
                    "when": "view == rhamtExplorerView && viewItem =~ /issue/i",
                    "group": "rhamt@2"
                },
                {
                    "command": "rhamt.Kai-Fix-Files",
                    "when": "view == rhamtExplorerView && viewItem =~ /file/i",
                    "group": "rhamt@0"
                },
                {
                    "command": "rhamt.Stop",
                    "when": "view == rhamtExplorerView && viewItem =~ /file/i",
                    "group": "rhamt@0"
                },
                {
                    "command": "rhamt.activate",
                    "when": "view == rhamtExplorerView && viewItem =~ /hasResults-notActive/i",
                    "group": "inline"
                },
                {
                    "command": "rhamt.deactivate",
                    "when": "view == rhamtExplorerView && viewItem =~ /hasResults-isActive/i",
                    "group": "inline"
                }
            ]
        },
        "colors": [
            {
                "id": "kaiFix.analyzing",
                "description": "Color for analyzing icon",
                "defaults": {
                    "light": "#0037ff",
                    "dark": "#0037ff"
                }
            },
            {
                "id": "kaiFix.fixing",
                "description": "Color for fixing icon",
                "defaults": {
                    "light": "#4CAF50",
                    "dark": "#4CAF50"
                }
            }
        ]
   },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/cheerio": "0.22.10",
        "@types/fs-extra": "^7.0.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^7.0.43",
        "@types/sinon": "^5.0.7",
        "@types/sinon-chai": "^3.2.2",
        "@types/socket.io": "^1.4.36",
        "@types/socket.io-client": "^1.4.32",
        "chai": "^4.2.0",
        "sinon": "^7.2.7",
        "sinon-chai": "^3.3.0",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.9.10",
        "vscode": "^1.1.33"
    },
    "dependencies": {
        "@eclipse-che/plugin": "latest",
        "@types/jquery": "^3.3.30",
        "cheerio": "1.0.0-rc.3",
        "express": "^4.17.1",
        "find-java-home": "^1.2.2",
        "fs-extra": "^8.0.1",
        "jade": "^1.11.0",
        "js-yaml": "^4.1.0",
        "jsonc-parser": "^2.1.0",
        "marked": "^12.0.1",
        "mime-types": "^2.1.26",
        "mkdirp": "^0.5.1",
        "net": "^1.0.2",
        "node-fetch": "^2.7.0",
        "node-stream-zip": "^1.8.0",
        "open": "^7.0.3",
        "opn": "^5.5.0",
        "request": "^2.88.2",
        "request-progress": "^3.0.0",
        "rimraf": "^3.0.2",
        "serve-static": "^1.14.1",
        "showdown": "^1.9.0",
        "socket.io": "^2.1.1",
        "socket.io-client": "^2.1.1",
        "tmp": "^0.1.0",
        "vscode-nls": "^2.0.2",
        "yaml": "^2.5.0"
    }
}